const OddishVsEvenish = (n) => {
    const MOV = ["Shimmy", "Shake", "Pirouette", "Slide", "Box Step", "Headspin", "Dosado", "Pop", "Lock", "Arabesque"];
    let cifra1 = n.toString();
    let cifra2 = 0;
    let resultado = "Evenish";
  
      for (let i = 0; i < cifra1.length; i++) {
          cifra2 += parseInt(cifra1[i]);
      }
      if (cifra2 % 2 === 1) {
          resultado = "Oddish";
      }  return resultado;
  }
