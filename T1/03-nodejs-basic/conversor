function conversor(n){
    let minutos,horas,dias;

        minutos= n / 60;

        horas= minutos / 60;

    
        dias= horas / 24;


            console.log( n + " estos segundos son: " + Math.trunc(dias) + " Dias, " + Math.trunc(horas) + " Horas, " + Math.trunc(minutos) + " minutos ");
}

conversor(95300);